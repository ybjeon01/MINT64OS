#include "Page.h"

void kInitializePageTables(void) {
    PML4ENTRY *pstPML4TEntry;
    PDPTENTRY *pstPDPTEntry;
    PDENTRY *pstPDEntry;
    DWORD dwMappingAddress;
    int i;

    // create PML4 table
    // set all PML4 entry with 0 except first entry
    pstPML4Entry = (PML4ENTRY *) 0x100000;

}

