[BITS 32]

; disclose following code to C language
global kReadCPUID, kSwithcAndExecute64bitKernel

SECTION .text

; return CPUID
; PARAM: DWORD dwEAX, DWORD *pdwEAX, *pdwEBX, *pdwECX, *pdwEDX
kReadCPUID:
    push ebp
    push ebp, esp
    push eax
    push ebx
    push ecx
    push edx
    push esi
